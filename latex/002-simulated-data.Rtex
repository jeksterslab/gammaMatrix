  )
)
@

<<>>=
set.seed(42)
n_i <- 100
mu_i <- c(0, 0)
k_i <- length(mu_i)
sigmacap_i <- matrix(
  data = c(
    1.0,
    0.5,
    0.5,
    1
  ),
  nrow = k_i,
  ncol = k_i
)
diag(sigmacap_i) <- 1
x_i <- matrix(
  data = rnorm(
    n = n_i * k_i
  ),
  nrow = n_i,
  ncol = k_i
) %*% (
  chol(sigmacap_i)
) + (
  matrix(
    data = 1,
    nrow = n_i,
    ncol = 1
  ) %*% mu_i
)
@

\section{General Formula}

<<>>=
gammacap_gen(x_i)
gammacap(
  x_i,
  type = "gen"
)
@

\section{Normally Distributed Data}

<<>>=
gammacap_mvn(x_i)
gammacap(
  x_i,
  type = "mvn"
)
gammacap_mvn(sigmacap = cov(x_i))
gammacap(
  sigmacap = cov(x_i),
  type = "mvn"
)
@

\section{Asymptotic Distribution-Free}

<<>>=
gammacap_adf(
  x_i,
  unbiased = TRUE
)
gammacap(
  x_i,
  type = "adf",
  unbiased = TRUE
)
gammacap_adf(
  x_i,
  unbiased = FALSE
)
gammacap(
  x_i,
  type = "adf",
  unbiased = FALSE
)
@

\section{Nonparametric Bootstrapped Covariances}

<<>>=
seed_i <- 42
bcap_i <- 5000L
gammacap_nb(
  x_i,
  bcap = bcap_i,
  seed = seed_i
)
gammacap(
  x_i,
  type = "nb",
  bcap = bcap_i,
  seed = seed_i
)
@

\section{Nonparametric Bootstrapped Asymptotic Distribution-Free}

<<>>=
seed_i <- 42
bcap_i <- 5000L
gammacap_adfnb(
  x_i,
  bcap = bcap_i,
  seed = seed_i
)
gammacap(
  x_i,
  type = "adfnb",
  bcap = bcap_i,
  seed = seed_i
)
@

\section{OLS}

<<>>=
x_i <- as.data.frame(x_i)
colnames(x_i) <- c("y", "x")
obj_i <- lm(
  y ~ x,
  data = x_i
)
gammacap_ols(obj_i)
@

\section{OLS-HC}

<<>>=
x_i <- as.data.frame(x_i)
colnames(x_i) <- c("y", "x")
obj_i <- lm(
  y ~ x,
  data = x_i
)
gammacap_ols_hc(obj_i)
@

